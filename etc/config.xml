<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <country>
            <redirect>
                <enabled>1</enabled>
                <ignore_actions>stores/store/redirect,stores/store/switch,stores/country/redirect</ignore_actions>
                <ignore_user_agents>googlebot,chrome-lighthouse,bingbot,slurp,duckduckbot,baiduspider,yandexbot,facebot,ia_archiver,yahoo,msnbot,nitro</ignore_user_agents>
            </redirect>
        </country>
    </default>
</config>
